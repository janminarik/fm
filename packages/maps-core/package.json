{
  "name": "@repo/maps-core",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "prisma"
  ],
  "scripts": {
    "build": "tsc -b -v",
    "clean": "pnpm dlx rimraf node_modules dist .turbo tsconfig.tsbuildinfo",
    "dev": "pnpm build --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" ",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "test": "jest --testPathIgnorePatterns=test/integration",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest test/integration",
    "test:watch": "jest --watch",
    "use-dev-env": "dotenv -e ../../.env.development --",
    "use-e2e-env": "dotenv -e ../../.env.test --"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.11",
    "@repo/config-eslint": "workspace:*",
    "@repo/config-typescript": "workspace:*",
    "cheerio": "^1.0.0",
    "pino": "^9.6.0",
    "pino-loki": "^2.5.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^22.4.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/testing": "^11.0.11",
    "@repo/config-eslint": "workspace:*",
    "@repo/config-jest": "workspace:*",
    "@repo/config-typescript": "workspace:*",
    "@types/pino-pretty": "^5.0.0",
    "@types/puppeteer": "^7.0.4",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.5.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typescript": "5.8.2"
  }
}