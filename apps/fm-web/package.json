{
  "name": "fm-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm tsc -b && vite build && pnpm run i18n-copy",
    "build:staging": "vite build --mode staging",
    "clean": "pnpm dlx rimraf node_modules dist .turbo",
    "dev": "npm run i18n-generate-types && npm run i18n-copy && concurrently \"npm run i18n-watch\" \"vite\"",
    "i18n-copy": "cpx \"src/i18n/translations/**/*\" public/i18n/translations",
    "i18n-generate-types": "i18next-resources-for-ts interface -i ./src/i18n/translations -o ./src/i18n/resources.d.ts",
    "i18n-watch": "cpx \"src/i18n/translations/**/*\" public/i18n/translations --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/utils": "^6.4.8",
    "@mui/x-data-grid": "^7.28.0",
    "@reduxjs/toolkit": "^2.6.1",
    "axios": "^1.8.3",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.3.0",
    "react-use": "^17.6.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@faker-js/faker": "^9.6.0",
    "@repo/config-eslint": "workspace: *",
    "@repo/config-typescript": "workspace:*",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cpx": "^1.5.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "i18next-resources-for-ts": "^1.5.0",
    "json-server": "1.0.0-beta.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2"
  }
}
